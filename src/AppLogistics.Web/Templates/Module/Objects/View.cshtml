@model ModuleModel
using System;
using System.ComponentModel.DataAnnotations;

namespace AppLogistics.Objects
{
    public class @Model.View : BaseView
    {
    @foreach (PropertyInfo property in Model.Properties)
    {
        @foreach (CustomAttributeData attribute in property.CustomAttributes)
        {
        @:[@attribute.AttributeType.Name.Replace("Attribute", "")]
        }
        @:public @property.PropertyType.Name @property.Name { get; set; }

    }
    }
}
